{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Https": {
        "Url": "https://*:8081",
        "Certificate": {
          "Path": "/app/cert/cert.pfx",
          "Password": "PLACEHOLDER_ENV_PASSWORD"
        }
      }
    }
  },
  "AllowedHosts": "*"
}
